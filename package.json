{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "jasmine": "jasmine",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "tsc": "tsc",
    "start": "cross-env ENV=dev tsc && node dist/src/server.js",
    "migrations": "db-migrate",
    "test": "db-migrate up -e test && cross-env ENV=test npm run build && npm run jasmine && db-migrate down -e test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
