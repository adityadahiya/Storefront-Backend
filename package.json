    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jasmine": "^3.8.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "request": "^2.88.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.9",
