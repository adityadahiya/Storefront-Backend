    "@types/request": "^2.48.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
